},{"filePath":"92","messages":"93","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},"/Users/macbook/Downloads/devfolio/ReactJs/src/index.js",[],["94","95"],"/Users/macbook/Downloads/devfolio/ReactJs/src/components/NavBar.js",[],"/Users/macbook/Downloads/devfolio/ReactJs/src/app.js",[],"/Users/macbook/Downloads/devfolio/ReactJs/src/App.js",[],"/Users/macbook/Downloads/devfolio/ReactJs/src/components/Footer.js",[],"/Users/macbook/Downloads/devfolio/ReactJs/src/components/Main.js",[],["96","97"],"/Users/macbook/Downloads/devfolio/ReactJs/src/components/Header.js",[],"/Users/macbook/Downloads/devfolio/ReactJs/src/components/CartWidget.js",[],"/Users/macbook/Downloads/devfolio/ReactJs/src/components/ItemListContainer.js",["98"],"import React, { useEffect, useState } from \"react\";\nimport ItemList from \"./ItemList\";\nimport { firestore } from \"../firebaseConfig\";\nimport { useParams } from \"react-router-dom\";\n\nfunction ItemListContainer() {\n  const { category } = useParams();\n  const [articulos, setArticulos] = useState([]);\n  \n  useEffect(() => {\n    obtenerArticulos.then((resultado) => {\n      if (category) {\n        const productosFiltrados = resultado.filter(\n          (articulos) => articulos.categoryId === category\n        );\n        setArticulos(productosFiltrados);\n      } else {\n        setArticulos(resultado);\n      }\n    });\n  }, [category]);\n  const obtenerArticulos = new Promise((resolve, reject) => {\n    const db = firestore;\n    const collection = db.collection(\"articulos\");\n    const query = collection.get();\n    query\n    .then((result) => {\n      const collectionItems = result.docs.map((art) => ({\n        id: art.id,\n        ...art.data(),\n      }));\n        resolve(collectionItems);\n      })\n      .catch((error) => {\n          reject(error);\n      });\n  });\n  return (\n    <div>\n      {category ? (\n        <h3>Resultado de busqueda... {category}  </h3>\n      ) : (\n          <> </>\n        )}\n   \n        <ItemList articulos={articulos} />\n     \n    </div>\n  );\n}\n\nexport default ItemListContainer;","/Users/macbook/Downloads/devfolio/ReactJs/src/components/Item.js",[],"/Users/macbook/Downloads/devfolio/ReactJs/src/components/ItemList.js",[],"/Users/macbook/Downloads/devfolio/ReactJs/src/components/InfoProductos.js",[],"/Users/macbook/Downloads/devfolio/ReactJs/src/components/ItemDetailContainer.js",[],"/Users/macbook/Downloads/devfolio/ReactJs/src/components/ItemDetail.js",["99"],"import React, { useState, useContext } from \"react\";\nimport ItemCount from \"./ItemCount\";\nimport \"bootstrap/dist/css/bootstrap.min.css\";\nimport { Link } from \"react-router-dom\";\nimport { CartContext } from \"./CartContext\";\n\n\nfunction ItemDetail({ item }) {\n\n  const { addItem } = useContext(CartContext);\n\n  const [stock, setStock] = useState(5);\n\n  const [sumado, setSumado] = useState(false);\n\n  const { title, price, pictureUrl, description, categoryId } = item;\n\n  const sumarAlCarrito = (cantidadSeleccionada) => {\n    if (stock >= cantidadSeleccionada) {\n      addItem({\n        item: item,\n        cantidad: cantidadSeleccionada,\n      });\n      setStock(stock - cantidadSeleccionada);\n      setSumado(true);\n    }\n  };\n\n\n  return (\n    <>\n      <div className=\"body-itemdetail\">\n        <nav aria-label=\"breadcrumb\">\n          <ol className=\"breadcrumb\">\n            <li className=\"breadcrumb-item\">\n              <Link to=\"/\">Inicio</Link>\n            </li>\n            <li className=\"breadcrumb-item active\" aria-current=\"page\">\n              {categoryId}\n            </li>\n            <li className=\"breadcrumb-item active\" aria-current=\"page\">\n              {title}\n            </li>\n          </ol>\n        </nav>\n        <div className=\"info-detail\">\n          <img src={pictureUrl} className=\"img-detail rounded float-left\" />\n          <h1> {title} </h1>\n          <p className=\"price-detail\"> ${price} </p>\n          <p className=\" text-detail text-lg-left\">{description}</p>\n          {sumado ? (\n            <div>\n              <Link to=\"/cart\" className=\"btn btn-primary\">\n               \n                Finalizar compra\n              </Link>\n            </div>\n          ) : (\n            <div className=\"precio\">\n              <ItemCount stock={stock} initial=\"0\" onAdd={sumarAlCarrito} />\n            </div>\n          )}\n        </div>\n      </div>\n    </>\n  );\n}\n\nexport default ItemDetail;\n","/Users/macbook/Downloads/devfolio/ReactJs/src/components/ItemCount.js",[],"/Users/macbook/Downloads/devfolio/ReactJs/src/components/Inicio.js",[],"/Users/macbook/Downloads/devfolio/ReactJs/src/components/Contacto.js",[],"/Users/macbook/Downloads/devfolio/ReactJs/src/components/CartContext.js",[],"/Users/macbook/Downloads/devfolio/ReactJs/src/components/Cart.js",[],"/Users/macbook/Downloads/devfolio/ReactJs/src/components/CartItem.js",[],"/Users/macbook/Downloads/devfolio/ReactJs/src/firebaseConfig.js",[],"/Users/macbook/Downloads/devfolio/ReactJs/src/components/Firebase.js",[],{"ruleId":"100","replacedBy":"101"},{"ruleId":"102","replacedBy":"103"},{"ruleId":"100","replacedBy":"104"},{"ruleId":"102","replacedBy":"105"},{"ruleId":"106","severity":1,"message":"107","line":21,"column":6,"nodeType":"108","endLine":21,"endColumn":16,"suggestions":"109"},{"ruleId":"110","severity":1,"message":"111","line":47,"column":11,"nodeType":"112","endLine":47,"endColumn":77},"no-native-reassign",["113"],"no-negated-in-lhs",["114"],["113"],["114"],"react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'obtenerArticulos'. Either include it or remove the dependency array.","ArrayExpression",["115"],"jsx-a11y/alt-text","img elements must have an alt prop, either with meaningful text, or an empty string for decorative images.","JSXOpeningElement","no-global-assign","no-unsafe-negation",{"desc":"116","fix":"117"},"Update th